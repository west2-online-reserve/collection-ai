    "\n",
    "# 导入数据集\n",
    "fmri = sns.load_dataset(\"fmri\")\n",
    "fmri\n",
    "\n",
    "# 绘制不同地区不同时间 x和y的线性关系图\n",
    "sns.lineplot(x=\"timepoint\", y=\"signal\",\n",
    "             hue=\"region\", style=\"event\",\n",
    "             data=fmri)\n",
    "\n",
    "\n"
