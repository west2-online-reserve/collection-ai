   "source": [
    "import numpy as np\n",
    "def softmax(z):\n",
    "    return np.exp(z) / np.sum(np.exp(z))\n",
    "\n",
    "z = np.array([1, 2, 3])\n",
    "z.shape,softmax(z).shape"
